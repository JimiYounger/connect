{
  "compilerOptions": {
    "target": "es2021",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["es2021", "dom"],
    "types": ["@supabase/supabase-js"],
    "skipLibCheck": true,
    // Ensure TypeScript doesn't complain about Deno URL imports
    "allowJs": true,
    "checkJs": false,
    "noEmit": true
  },
  "include": ["./**/*.ts", "edge-functions.d.ts"]
} 