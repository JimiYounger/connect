{
  "name": "Connect by Purelight",
  "short_name": "Connect",
  "description": "Everything You Need In One Place",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#000000",
  "theme_color": "#000000",
  "display_override": ["window-controls-overlay"],
  "protocol_handlers": [
    {
      "protocol": "web+connect",
      "url": "/?deeplink=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://plpconnect.com/manifest.json"
    }
  ],
  "icons": [
    {
      "src": "/connect.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png"
    },
    {
      "src": "/apple-touch-icon.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/connect.png",
      "sizes": "192x192",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "screenshots": [
    {
      "src": "/pwa-screenshot.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "url": "/dashboard",
      "icons": [{ "src": "/connect.png", "sizes": "192x192" }]
    }
  ],
  "id": "com.purelight.connect"
}